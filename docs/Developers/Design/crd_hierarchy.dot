# dot -Tpng -O <this_file>

digraph {
  label="Hierarchy of Monstorak Custom Resources"
  fontname="helvetica"
  nodesep=0.5

  node [
    shape=rect
    fontname="helvetica"
  ]

  edge [
    dir=forward
    fontname="helvetica"
  ]

  o1 [label="Instance: Operator"]
  c11 [label="Controller 1"]
  c12 [label="Controller 2"]

  o1->c11, c12

  cr1 [label="StorageAlert"]
  cr2 [label="StorageMonitoringPlan"]

  c11->cr1
  c12->cr2
}
